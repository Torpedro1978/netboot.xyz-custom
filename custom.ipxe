#!ipxe

goto ${menu} ||

:custom_menu
menu Torpedro1978 custom menu
item --gap OS:
item windows ${space} Windows
choose menu || goto custom_exit
echo ${cls}
goto ${menu} ||
goto change_menu

:change_menu
chain --autofree https://raw.githubusercontent.com/Torpedro1978/netboot.xyz-custom/master/${menu}.ipxe || goto error
goto custom_menu

:custom_exit
exit 0
